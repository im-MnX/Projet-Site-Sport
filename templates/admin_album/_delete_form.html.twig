<form method="post" action="{{ path('app_admin_album_delete', {'idAlbum': album.idAlbum}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ album.idAlbum) }}">
    <button class="btn btn-danger"><i class="fa-solid fa-trash"></i> Supprimer</button>
</form>
